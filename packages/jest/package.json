{
  "name": "@emotion/jest",
  "version": "12.0.0-next.0",
  "description": "Jest utilities for emotion",
  "main": "dist/emotion-jest.cjs.js",
  "module": "dist/emotion-jest.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/emotion-jest.cjs.mjs",
        "default": "./dist/emotion-jest.cjs.js"
      },
      "module": "./dist/emotion-jest.esm.js",
      "import": "./dist/emotion-jest.cjs.mjs",
      "default": "./dist/emotion-jest.cjs.js"
    },
    "./serializer": {
      "types": {
        "import": "./serializer/dist/emotion-jest-serializer.cjs.mjs",
        "default": "./serializer/dist/emotion-jest-serializer.cjs.js"
      },
      "module": "./serializer/dist/emotion-jest-serializer.esm.js",
      "import": "./serializer/dist/emotion-jest-serializer.cjs.mjs",
      "default": "./serializer/dist/emotion-jest-serializer.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "types": "types/index.d.ts",
  "files": [
    "src",
    "dist",
    "types/*.d.ts",
    "serializer",
    "enzyme",
    "enzyme-serializer"
  ],
  "scripts": {
    "test:typescript": "dtslint types"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3",
    "@emotion/css-prettifier": "^1.2.0",
    "chalk": "^4.1.0",
    "specificity": "^0.4.1",
    "stylis": "4.2.0"
  },
  "peerDependencies": {
    "@types/jest": "^26.0.14 || ^27.0.0 || ^28.0.0 || ^29.0.0",
    "enzyme-to-json": "^3.2.1"
  },
  "peerDependenciesMeta": {
    "@types/jest": {
      "optional": true
    },
    "enzyme-to-json": {
      "optional": true
    }
  },
  "devDependencies": {
    "@definitelytyped/dtslint": "0.0.112",
    "@emotion/css": "12.0.0-next.0",
    "@emotion/react": "12.0.0-next.0",
    "@types/jest": "^29.5.12",
    "enzyme-to-json": "^3.6.1",
    "pretty-format": "^22.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "strip-ansi": "^6.0.1",
    "typescript": "^5.4.5"
  },
  "author": "Kye Hohenberger",
  "homepage": "https://emotion.sh",
  "license": "MIT",
  "repository": "https://github.com/emotion-js/emotion/tree/main/packages/jest",
  "keywords": [
    "styles",
    "emotion",
    "react",
    "css",
    "css-in-js",
    "jest",
    "snapshot"
  ],
  "bugs": {
    "url": "https://github.com/emotion-js/emotion/issues"
  },
  "preconstruct": {
    "entrypoints": [
      "./index.js",
      "./serializer.js"
    ]
  }
}
