{
  "name": "@emotion/css-prettifier",
  "version": "1.2.0",
  "description": "Simple Stylis-based CSS prettifier",
  "keywords": [
    "emotion"
  ],
  "author": "Mateusz Burzy≈Ñski <mateuszburzynski@gmail.com> (https://github.com/Andarist)",
