{
    "extends": "dtslint/dtslint.json",
    "rules": {
        "only-arrow-functions": false,

        "array-type": [
            true,
            "generic"
        ],
        "semicolon": false,
        "space-before-function-paren": false,
        "no-null-undefined-union": false,
        "callable-types": false
    }
}