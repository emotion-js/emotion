{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "../",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "target": "es5",
    "typeRoots": [
      "../"
    ],
    "types": []
  },
  "include": [
    "./*.ts",
    "./*.tsx"
  ]
}
