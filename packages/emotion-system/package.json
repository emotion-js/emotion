    "react-emotion": "^8.0.2-5",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0"
  },
  "author": "Kye Hohenberger",
  "homepage": "https://github.com/tkh44/emotion#readme",
  "license": "MIT",
