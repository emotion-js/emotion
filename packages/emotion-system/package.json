{
  "name": "emotion-system",
  "version": "8.0.2-5",
  "description": "emotion and styled-system",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clean rollup rollup:umd",
