{
  "compilerOptions": {
    "baseUrl": "../",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "es6",
      "dom"
    ],
    "declaration": true,
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "target": "es5",
    "typeRoots": [
      "../"
    ],
    "types": []
  },
  "include": [
    "./src/*.ts",
    "./src/*.tsx"
  ]
}
