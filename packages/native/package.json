{
  "name": "@emotion/native",
  "version": "11.11.0",
  "description": "Style and render React Native components using emotion",
  "main": "dist/emotion-native.cjs.js",
  "module": "dist/emotion-native.esm.js",
  "exports": {
    ".": {
      "module": "./dist/emotion-native.esm.js",
      "import": "./dist/emotion-native.cjs.mjs",
      "default": "./dist/emotion-native.cjs.js"
    },
    "./package.json": "./package.json",
    "./macro": {
      "types": {
        "import": "./macro.d.mts",
        "default": "./macro.d.ts"
      },
      "default": "./macro.js"
    }
  },
  "scripts": {
    "test:typescript": "dtslint types"
  },
  "files": [
    "src",
    "dist",
    "types/*.d.ts",
    "macro.*"
  ],
  "types": "types/index.d.ts",
  "devDependencies": {
    "@definitelytyped/dtslint": "0.0.112",
    "@types/react-native": "^0.63.2",
    "react": "16.14.0",
    "react-native": "^0.63.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@emotion/primitives-core": "^11.11.0"
  },
  "peerDependencies": {
    "react-native": ">=0.14.0 <1"
  },
  "homepage": "https://emotion.sh",
  "license": "MIT",
  "repository": "https://github.com/emotion-js/emotion/tree/main/packages/native",
  "keywords": [
    "styles",
    "emotion",
    "react",
    "css",
    "css-in-js",
    "native"
  ],
  "bugs": {
    "url": "https://github.com/emotion-js/emotion/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "exports": {
      "extra": {
        "./macro": {
          "types": {
            "import": "./macro.d.mts",
            "default": "./macro.d.ts"
          },
          "default": "./macro.js"
        }
      }
    }
  }
}
