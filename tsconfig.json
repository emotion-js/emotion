{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "noEmit": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["packages", "scripts"],
  "exclude": ["node_modules"]
}
