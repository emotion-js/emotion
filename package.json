{
  "private": true,
  "name": "emotion-monorepo",
  "version": "10.0.0",
  "scripts": {
    "build:watch": "preconstruct watch",
    "build": "preconstruct build",
    "test:size": "npm-run-all build size",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test": "jest",
    "test:typescript": "yarn workspaces run test:typescript",
    "coverage": "jest --coverage --no-cache --ci --runInBand",
    "test:prod": "yarn build && jest -c jest.dist.js --no-cache --ci --runInBand",
    "lint:check": "eslint .",
    "test:watch": "jest --watch",
    "size": "bundlesize",
    "lint": "eslint . --fix",
    "benchmark": "cd scripts/benchmarks && yarn benchmark",
    "start:site": "cd site && yarn start:site",
    "build:site": "cd site && yarn build:site",
    "flow": "flow",
    "flow:check": "flow check --flowconfig-name=.flowconfig-ci",
    "postinstall": "opencollective postinstall && preconstruct dev && bolt-check",
    "changeset": "changeset",
    "version-packages": "changeset bump",
    "release": "yarn build && changeset release"
  },
  "resolutions": {
    "**/react": "16.8.1",
    "**/react-dom": "16.8.1",
    "**/browserslist": "^3.2.8",
    "**/graphql-type-json": "0.2.4"
  },
  "author": "Kye Hohenberger",
  "homepage": "https://emotion.sh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emotion-js/emotion.git"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [
    "styles",
    "emotion",
    "react",
    "css",
    "css-in-js"
  ],
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier",
      "flowtype"
    ],
    "parser": "babel-eslint",
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": 0,
      "react/no-unused-prop-types": 0,
      "standard/computed-property-even-spacing": 0,
      "no-template-curly-in-string": 0,
      "camelcase": 0,
      "import/no-duplicates": 0,
      "flowtype/define-flow-type": 2
    },
    "env": {
      "browser": true
    },
    "overrides": [
      {
        "files": [
          "*.test.js",
          "**/__tests__/**"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/packages/*/src/*",
          "**/packages/*/src/**/*"
        ],
        "rules": {
          "import/no-commonjs": 2
        }
      },
      {
        "files": [
          "**/__fixtures__/*"
        ],
        "rules": {
          "no-unused-vars": 0
        },
        "env": {
          "jest": false
        }
      }
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "site",
      "scripts/*",
      "playgrounds/*"
    ],
    "nohoist": [
      "**/dtslint"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ],
    "globals": {
      "react": "React"
    }
  },
  "bugs": {
    "url": "https://github.com/emotion-js/emotion/issues"
  },
  "bundlesize": [
    {
      "path": "./packages/emotion/dist/emotion.umd.min.js",
      "threshold": "7 Kb"
    },
    {
      "path": "./packages/styled-base/dist/styled-base.umd.min.js",
      "threshold": "9.7 Kb"
    },
    {
      "path": "./packages/core/dist/core.umd.min.js",
      "threshold": "7 Kb"
    }
  ],
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/emotion"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "@changesets/cli": "^1.3.0",
    "@changesets/get-github-info": "^0.1.3",
    "@mdx-js/mdx": "^1.1.0",
    "@mdx-js/react": "^1.0.27",
    "@testing-library/react": "^8.0.2",
    "@types/jest": "^23.0.2",
    "@types/node": "^10.11.4",
    "@types/react": "^16.8.20",
    "babel-check-duplicated-nodes": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-flow-types": "^1.2.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-add-basic-constructor-for-react-components": "^0.1.0",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-codegen": "^3.0.0",
    "babel-plugin-fix-dce-for-classes-with-statics": "^0.1.0",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-macros": "^2.0.0",
    "babel-plugin-react-native-web": "^0.9.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-standalone": "^6.26.0",
    "benchmark": "^2.1.4",
